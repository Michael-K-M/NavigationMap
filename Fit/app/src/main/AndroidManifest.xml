        <activity android:name="com.example.NavigationMap.Login" />
        <activity android:name="com.example.NavigationMap.Sign_up" />
        <activity android:name="com.example.NavigationMap.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER"/>
